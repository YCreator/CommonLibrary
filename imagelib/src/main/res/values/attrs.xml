<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BigImageView">
        <attr format="enum" name="initScaleType">
            <enum name="centerInside" value="1"/>
            <enum name="centerCrop" value="2"/>
            <enum name="auto" value="3"/>
        </attr>
        <attr format="boolean" name="optimizeDisplay"/>
    </declare-styleable>

    <declare-styleable name="CircleLoadingView">
        <attr name="startColor" format="color" />
        <attr name="endColor" format="color" />
        <attr name="circleThicknessRatio" format="float" />
    </declare-styleable>
</resources>